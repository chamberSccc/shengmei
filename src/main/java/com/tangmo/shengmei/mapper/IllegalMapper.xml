<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tangmo.shengmei.dao.IllegalDao">

    <resultMap id="BaseResultMap" type="com.tangmo.shengmei.entity.IllegalInfo" >
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="user_id" property="userId" jdbcType="INTEGER"/>
        <result column="car_id" property="carId" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="insertSelective" parameterType="com.tangmo.shengmei.entity.AdminInfo">
        insert into admin_info (account,password) VALUES (#{account},#{password})
    </insert>

    <select id="selectByUserAndCar" resultMap="AdminResultMap">
        SELECT id,user_id,car_id FROM illegal_info WHERE user_id = #{userId} and car_id = #{carId}
    </select>

</mapper>